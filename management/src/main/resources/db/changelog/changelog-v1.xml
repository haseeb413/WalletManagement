    </changeSet>
    <changeSet id="2" author="Dickson the developer">
        <addColumn schemaName="wallet"
                   tableName="walled">
            <column name="balance" type="varchar(255)" />
        </addColumn>
    </changeSet>


</databaseChangeLog>